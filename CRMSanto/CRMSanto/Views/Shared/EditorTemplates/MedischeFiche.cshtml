@model CRMSanto.Models.MedischeFiche
@{
    List<CRMSanto.Models.Mutualiteit> list = (List<CRMSanto.Models.Mutualiteit>)ViewData["Mutualiteiten"];
}

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

<div class="row">
    <div class="input-field col s6">
        @Html.EditorFor(model => model.Voedingspatroon, new { htmlAttributes = new { @class = "validate", id = "voedingspatroon", placeholder = "" } })
        <label for="voedingspatroon">@Html.DisplayNameFor(model => model.Voedingspatroon)</label>
    </div>
    <div class="input-field col s6">
        @Html.EditorFor(model => model.Spijsvertering, new { htmlAttributes = new { @class = "validate", id = "spijsvertering", placeholder = "" } })
        <label for="spijsvertering">@Html.DisplayNameFor(model => model.Spijsvertering)</label>
    </div>
</div>

<div class="row">
    <div class="input-field col s6">
        @Html.EditorFor(model => model.HuidigeKlachten, new { htmlAttributes = new { @class = "validate", id = "huidigeklachten", placeholder = "" } })
        <label for="huidigeklachten">@Html.DisplayNameFor(model => model.HuidigeKlachten)</label>
    </div>
    <div class="input-field col s6">
        <select id="MedischeFiche_Mutualiteit_ID" name="MedischeFiche.Mutualiteit.ID">
                @foreach (var m in list) 
                {  
                    <option value="@m.ID">@m.Naam</option>
                } 
        </select>
        <label for="MedischeFiche_Mutualiteit_ID">@Html.DisplayNameFor(model => model.Mutualiteit)</label>
    </div>
</div>